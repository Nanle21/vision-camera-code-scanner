{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "react-native-vision-camera": ["node_modules/react-native-vision-camera/src"],
      "vision-camera-code-scanner": ["./src/index"]
    },
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "esModuleInterop": true,
    "importsNotUsedAsValues": "remove", // Deprecated, but needs to be set to "remove",
    "verbatimModuleSyntax": true, // Add this line to use 'verbatimModuleSyntax',
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "types": ["node"],
    "lib": ["esnext"],
    "module": "esnext",
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noImplicitUseStrict": false,
    "noStrictGenericChecks": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "esnext"
  }
}
